{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "outDir": "build",
    "rootDir": ".",
    "target": "ES2019",
    "lib": ["es2019", "es2020.promise", "es2020.bigint", "es2020.string"],
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts" ]
}
