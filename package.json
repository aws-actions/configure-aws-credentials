  "devDependencies": {
    "@aws-sdk/credential-provider-env": "^3.40.0",
    "@aws-sdk/credential-provider-web-identity": "^3.41.0",
    "@types/async-retry": "^1.4.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.4",
    "@vercel/ncc": "^0.33.1",
    "aws-sdk-client-mock": "^0.5.6",
    "eslint-plugin-jest": "^25.3.0",
    "gts": "^3.1.0",
